{
	"name": "generic-website",
	"private": true,
	"workspaces": {
		"packages": [
			"server-api",
			"server-data",
			"server-web"
		],
		"nohoist": [
			"**"
		]
	},
	"scripts": {
		"api": "cd server-api && yarn run start",
		"database": "cd server-data && yarn run start",
		"web": "cd server-web && yarn run build"
	},
	"installConfig": {
		"hoistingLimits": "workspaces",
		"pnp": false
	}
}
